@model DealEngine.WebUI.Models.Authorization.AuthorizeViewModel

<form>
    <div class="form-group col-6">
        <select class="custom-select" id="DeleteRoleSelect">
            @foreach (var role in Model.RoleList)
            {
                <option value="@role.Name">@role.Name</option>
            }
        </select>
    </div>
   
    <div class="form-group col-6">
        <button type="button" class="btn btn-primary btn-lg" onclick="DeleteRole()">Delete</button>
    </div>
</form>


