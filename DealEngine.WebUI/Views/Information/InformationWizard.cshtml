                                                                                        <option id="MooredTypeMarina" value="Marina">Marina</option>
                                                                                        <option value="Swing">Swing</option>
                                                                                    </select>
                                                                                </label>
                                                                            </section>
                                                                        </div>
                                                                    </div>
                                                                    <div class="modal-footer">
                                                                        <section class="col col-xs-12">
                                                                            <button onclick="AddWaterLocModal('#WaterLocationNameModal','#WaterLocationLatitudeModal', '#WaterLocationLongitudeModal', '#WaterLocationLocationModal', '#WaterLocationMarinaLocationModal', '#MooredTypeModal','#OrgUnitModal'); $('#addWaterLocModal').modal('hide');" class="btn btn-success">Add</button>
                                                                        </section>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
