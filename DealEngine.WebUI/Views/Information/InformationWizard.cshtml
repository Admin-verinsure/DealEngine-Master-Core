@section pagespecific {
    <link rel="stylesheet" href="~/css/smartadmin-production.min.css">
    @await Html.PartialAsync("_InformationWizardScripts")
    @await Html.PartialAsync("_DataTableScripts")
    @*@await Html.PartialAsync("_PrefabScripts")*@
    <script>

        $(document).ready(function () {
            var accordianPanel = sessionStorage.getItem("accordianTrigger");
            if (accordianPanel != null) {
                var section = document.getElementById(accordianPanel);
                if (section.parentElement.id == "later") {
                    $("#more").trigger("click");
                }
                debugger;
                $("[id='" + accordianPanel + "']").trigger("click");
                sessionStorage.removeItem("accordianTrigger");
            }

            advisoryModal = document.getElementById('advisoryModal');
            if (advisoryModal !== null) {
                $("#advisoryModal").modal('show');
            }

            if ("@Model.Status" == "Submitted") {
                debugger;
                if (document.getElementById('fieldset') != null) {
                    document.getElementById('fieldset').setAttribute("disabled", "disabled");
                } else {
                    document.getElementById('btn1').removeAttribute("disabled");
                }
                $('.btn-sm').addClass("hidden");
                $("#accordion :input").prop("disabled", true);
                $('.btn-success').removeClass("hidden");
                $('.btn-success').prop("disabled", false);

            }
        });


        function CloseAdvisory(clientInformationSheetId) {
            if (typeof clientInformationSheetId !== null) {
                var postData = { ClientInformationSheetId: clientInformationSheetId };
                $.ajax({
                    url: '@Url.Action("CloseAdvisory", "Services")',
                    type: "GET",
                    data: postData
                })
                    .done(function (data) {
                        debugger;
                        document.location.href = "/";
                    })
                    .fail(function (a, s, d) {
                        debugger;
                    });
            }
            else {
                $('#advisoryModal').modal('hide');
            }
        }


    </script>

}

