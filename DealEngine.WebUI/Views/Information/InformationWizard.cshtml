                var section = document.getElementById(accordianPanel);
                if (section.parentElement.id == "later") {
                    $("#more").trigger("click");
                }
                $("[id='" + accordianPanel + "']").trigger("click");
                sessionStorage.removeItem("accordianTrigger");
            }

            advisoryModal = document.getElementById('advisoryModal');
            if ("@Model.Status" === "Not Started") {
                if ("@Model.Advisory" !== "") {
                    $("#advisoryModal").modal('show');
