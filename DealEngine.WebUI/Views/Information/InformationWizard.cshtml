@model DealEngine.WebUI.Models.InformationViewModel

<div id="content" style="background-color: #A6E2EF">
    <!-- widget grid -->
    <section id="widget-grid" class="">
        <!-- row -->
        <div class="row">
            <!-- NEW WIDGET START -->
            <article class="container">
                <!-- Widget ID (each widget will need unique ID)-->
                <div id="container" class="container-fluid">
                    <div aria-live="polite" aria-atomic="true">
                        <div class="toast" id="toast" style="position: fixed; top: 0; right: 0;" data-delay="3000">
                            <div class="toast-header">
                                <i class="fa fa-globe">
                                    <strong class="mr-auto"> Success </strong>
                                    <small class="text-muted"> just now </small>
                                    <button type="button" class="ml-2 mb-1 close" data-dismiss="toast" aria-label="Close">
                                        <span aria-hidden="true">&times;</span>
                                    </button>
                                </i>
                            </div>
                            <div class="toast-body" id="toastbody">
                            </div>
                        </div>
                    </div>
                    <div class="jarviswidget" id="wid-id-1" data-widget-colorbutton="false" data-widget-editbutton="false" data-widget-togglebutton="false"
                         data-widget-deletebutton="false" data-widget-custombutton="false" data-widget-sortable="false">

                        <header style="background-color:#EF4E45; color:white">
                            <h2>Get Insurance</h2>
                        </header>

                        @Html.HiddenFor(m => m.AnswerSheetId)
                        @Html.HiddenFor(m => m.Status)
                        @*@Html.HiddenFor(m => m.AgreementStatus)*@
                        @Html.HiddenFor(m => m.Id)
                        @Html.HiddenFor(m => m.ListProductName)

                        <div role="content">
                            <!-- widget content -->
                            @if (Model.ClientInformationSheet.Status == "Submitted")
                            {

                                <div class="alert alert-warning">
                                    This information sheet has already been submitted, and cannot be updated.
                                </div>

                            }
                            <div class="widget-body">

                                <form class="is-valid" id="RoleDataViewModel" data-accordianId="Roles"></form>
                                <form id="LocationViewModel"></form>
                                @*<form action="#" class="" id="myForm" role="form" data-toggle="validator" method="post" accept-charset="utf-8">*@

                                <!-- SmartWizard html -->
                                <div id="smartwizard" class="sw-main sw-theme-circles">
                                    <ul class="nav nav-tabs step-anchor">

                                        @for (int i = 0; i < Model.Wizardsteps.Count; i++)
                                        {
                                            var index = i + 1;
                                            if (i == 0)
                                            {
                                                <li class="nav-item active"><a href="#tab_@index" class="nav-link">@index<br><small>@Model.Wizardsteps.ElementAt(i)</small></a></li>
                                            }
                                            else
                                            {
                                                <li class="nav-item"><a href="#tab_@index" class="nav-link">@index<br><small>@Model.Wizardsteps.ElementAt(i)</small></a></li>
                                            }
                                        }
                                    </ul>

                                    <div class="sw-container tab-content">
                                        <div id="tab_1" class="tab-pane step-content" style="display: block;">
                                            <h3><strong> @Model.Wizardsteps.ElementAt(0) </strong> - Complete Information</h3>
                                            <div id="form-step-0" role="form" data-toggle="validator">
                                                <div class="form-group">
                                                    <div class="row">
                                                        @{ await Html.RenderPartialAsync("_ViewInformation"); }
                                                        <div class="modal" id="addOperatorModal" tabindex="-1" role="dialog" data-backdrop="static">
                                                            <div class="modal-dialog">
                                                                <div class="modal-content">
                                                                    <div class="modal-header">
                                                                        <button type="button" class="close" onclick="$('#addOperatorModal').hide();">&times;</button>
                                                                        <h4 class="modal-title">Add a Skipper</h4>
                                                                    </div>
                                                                    <div class="modal-body">
                                                                        <input id="OperatorId" name="OperatorId" type="hidden" />
                                                                        <div class="row">
                                                                            <section class="col col-xs-12 col-sm-6 col-md-6 col-lg-6">
                                                                                <label class="label cgmodalLabel required" style="color:black; font-size:100%;">Skipper First Name</label>
                                                                                <label class="input cgmodalInput">
                                                                                    <i></i>
                                                                                    <input class="form-control oper" type="text" data-type="TEXTBOX" data-name="OperatorFirstName" id="OperatorFirstNameModal" name="FirstName" placeholder="Skipper First Name" value="" required>
                                                                                </label>
                                                                                <p id="demo"></p>
                                                                            </section>
                                                                            <section class="col col-xs-12 col-sm-6 col-md-6 col-lg-6">
                                                                                <label class="label cgmodalLabel required" style="color:black; font-size:100%;">Skipper Last Name</label>
                                                                                <label class="input cgmodalInput">
                                                                                    <i></i>
                                                                                    <input class="form-control oper" type="text" data-type="TEXTBOX" data-name="OperatorLastName" id="OperatorLastNameModal" name="LastName" placeholder="Skipper Last Name" value="" required>
                                                                                </label>
                                                                            </section>
                                                                            <section class="col col-xs-12 col-sm-6 col-md-6 col-lg-6">
                                                                                <label class="label cgmodalLabel required" style="color:black; font-size:100%;">Skipper Email</label>
                                                                                <label class="input cgmodalInput">
                                                                                    <i></i>
                                                                                    <input class="form-control oper" type="text" data-type="TEXTBOX" data-name="Email" id="organisationEmail" name="Email" placeholder="Skipper Email" value="" required>
                                                                                </label>
                                                                            </section>
                                                                        </div>
                                                                    </div>
                                                                    <div class="modal-footer">
                                                                        <section class="col col-xs-12">
                                                                            <button onclick="validateOperatorModal('#OperatorFirstNameModal', '#OperatorLastNameModal','locTabs1', '#OperatorYearsOfExpModal','#organisationEmail')" class="btn btn-success">Add</button>
                                                                        </section>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="modal" id="addWaterLocModal" tabindex="-1" role="dialog" data-backdrop="static">
