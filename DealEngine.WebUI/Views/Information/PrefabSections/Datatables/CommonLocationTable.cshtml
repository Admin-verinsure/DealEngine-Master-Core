@model DealEngine.WebUI.Models.InformationViewModel

<table id="tblLocation"
       class="table table-striped" width="100%"
       data-show-columns="true"
       data-pagination="true"
       data-search="true"
       data-show-jump-to="true">
    <thead>
        <tr>
            <th data-field="editButton" data-sortable="false"></th>
            <th data-field="locationType" data-sortable="true">Location Type</th>
            <th data-field="commonName" data-sortable="true">Common Name</th>
            <th data-field="street" data-sortable="true">Street</th>
            <th data-field="suburb" data-sortable="true">Suburb</th>
            <th data-field="postcode" data-sortable="true">Postcode</th>
            <th data-field="city" data-sortable="true">City</th>
            <th ata-field="deleteButton" data-sortable="false"></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var location in Model.Locations)
        {
        <tr>
            <td>
                <button class="btn btn-success btn-sm" onclick="EditLocation('@location.LocationId')"><i class="fa fa-edit"></i></button>
            </td>
            <td>
            </td>
            <td>
            </td>
            <td>
            </td>
            <td>
            </td>
            <td>
            </td>
            <td>
            </td>
            <td>
            </td>
        </tr>
        }
</tbody>
    </table>

