@model DealEngine.WebUI.Models.InformationViewModel

    <form id="DAOLIViewModel" class="is-valid" data-accordianId="Directors and Officers Liability Insurance">
      
        <div class="form-group">
            <div class="row">
                <div class="col-9">
                    <label for="hasotherDO" class="label required">
                        Are you aware of any other D&O insurances held for any of the parties to this policy?
                    </label>
                </div>
                <div class="col-3">
                    <select id="hasotherDO" name="DAOLIViewModel.HasOtherDAOOptions" class="custom-select" onchange="OnChange(this.id, this.name)" required>
                        @foreach (var option in Model.DAOLIViewModel.HasOtherDAOOptions)
                        {
                            <option selected="@option.Selected" value="@option.Value">@option.Text</option>
                        }
                    </select>
                </div>
            </div>
        </div>
    </form>
