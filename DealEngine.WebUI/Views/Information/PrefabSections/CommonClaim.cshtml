                <fieldset id="listallfieldset">
                    <div class="row">
                        <section class="col col-xs-12">
                            @*<script src="~/jquery/jquery.min.js"></script>*@
                            <script type="text/javascript">
                                $(window).on('load',function () {
							    $("#tblClaim").jqGrid({
							        url: "/Services/GetClaims?informationId=@Model.ClientInformationSheet.Id&removed=false",
							        datatype: "xml",
                                    mtype: "GET",
                                    async: true,
							        colNames: ["","Claim Title", "Claim Description", "Claim Reference", "Name of Claimant", ""],
                                    colModel: [
                                        {
                                            name: "update", align: 'center', search: false, formatter: function (cellvalue, options, rowobject) {
                                                return ('<button onclick="EditClaim(\'' + cellvalue + '\', \'#tblClaim\'); SetCurrentTab(\'clTabs2\');" type="button"class="btn btn-success btn-sm"><i class="fa fa-edit"></i></button>');
                                            }
                                        },
							            { name: "ClaimTitle", align: 'center', key: 'true' },
                                        { name: "ClaimDescription", align: 'center' },
							            { name: "ClaimReference", align: 'center' },
							            { name: "Claimant", align: 'center' },
							            { name: "update", align: 'center', search: false, formatter: function(cellvalue, options, rowobject)
                                        {
                                            return ('<button onclick="SetClaimRemovedStatus(\'' + cellvalue + '\', \'true\', \'#tblClaim\', \'#tblRemovedClaim\');" type="button"class=" btn btn-danger btn-sm"><span class="fa fa-trash" id="span"></span></button>');
							            }
										}
									],
							        autowidth: true,
							        pager: "#pagerClaim",
							        rowNum: 10,
							        rowList: [10, 20, 30],
							        sortname: "ClaimTitle",
							        sortorder: "desc",
							        viewrecords: true,
							        gridview: true,
							        autoencode: true
							    });
                                $("#tblClaim").jqGrid('navGrid',
							    	'#pagerClaim',
							    	{edit: false, add: false, del: false},
							    	//options
							    	{height:280,reloadAfterSubmit:false},
							    	// edit options
							    	{height:280,reloadAfterSubmit:false},
							    	// add options
							    	{reloadAfterSubmit:false},
							    	// del options
						    		{sopt:['eq','bw','cn']}
							    	// search options
							    );
							});
                            </script>
                            <table id="tblClaim"><tr><td></td></tr></table>
                            <div id="pagerClaim"></div>
                        </section>
                    </div>
