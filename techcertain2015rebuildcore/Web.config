  <!-- Look into ElmahR Multiple server logging-->
  <elmah>
    <security allowRemoteAccess="true" />
    <errorLog type="Elmah.PgsqlErrorLog, Elmah" connectionStringName="TechCertainConnection" applicationName="Proposalonline.com" />
