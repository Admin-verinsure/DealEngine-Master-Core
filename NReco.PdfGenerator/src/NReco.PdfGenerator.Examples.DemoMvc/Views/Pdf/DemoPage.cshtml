@model System.Collections.IDictionary
	
	<div class="row">
		<div class="col-md-12">
			<div class="pull-left">
				<h1>
					PDF Generator for .NET (C#)
					<small>WkHtmlToPdf wrapper</small>
				</h1>
			</div>
			<div class="clearfix"></div>
		</div>
    </div>
	<hr />
	
	<div class="row">
		<div class="col-md-12">
			
			<a href="@Url.Action("GetPdf", "Pdf", new { id = "PageBreaks" })" class="btn btn-info btn-large">Page breaks / Repeat Table heading on each page</a>

			<a href="@Url.Action("GetPdf", "Pdf", new { id = "LargeContentInTableEmulation" })" class="btn btn-info btn-large">Large Content In Table Emulation</a>

			<br/><br/>

			<a href="@Url.Action("CoverAndToc", "Pdf")" class="btn btn-info btn-large">Cover (first page) and autogenerated Table of Contents (bookmarked)</a>
		
			<a href="@Url.Action("GetPdf", "Pdf", new { id = "HeaderAndFooter" } )" class="btn btn-info btn-large">Custom header and footer with page numbering</a>
	
		</div>
	</div>	
	
