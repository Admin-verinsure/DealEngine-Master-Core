                    $('#mnumb').disableSelection;
                    $('#vtype').attr('readonly', 'readonly');
                    $('#vtype').attr('disabled', 'true');
                    $('#ctype').attr('readonly', 'readonly');
                    $('#ctype').attr('disabled', 'true');
                    $('#hcfig').attr('readonly', 'readonly');
                    $('#hcfig').attr('disabled', 'true');
                    $('#vival').attr('readonly', 'readonly');
                    $('#vival').disableSelection;
                    $('#mtype').attr('readonly', 'readonly');
