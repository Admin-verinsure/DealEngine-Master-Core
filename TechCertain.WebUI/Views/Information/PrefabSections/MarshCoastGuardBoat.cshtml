@model TechCertain.WebUI.Models.InformationViewModel

@using System.Linq

<div class="padding-10">
    <div id="proposalcreate-form" class="smart-form" novalidate="novalidate">

        @*@tabs.Tab("List All")
            @tabs.Tab("Edit").Class("EditButton")
            @tabs.Tab("Add").Class("AddButton")
            @tabs.Tab("List Removed").Class("BoatRemoved")*@
        <div id="Boattabs">

            <ul id="tab" class="nav nav-tabs">
                <li class="nav nav-tabs active"> <a href="#boatTabs1"> List All </a> </li>
                <li class="nav nav-tabs EditButton"> <a href="#boatTabs2"> Edit </a> </li>
                <li class="nav nav-tabs AddButton"> <a href="#boatTabs3"> Add </a> </li>
                <li class="nav nav-tabs BoatRemoved"> <a href="#boatTabs4"> List Removed </a> </li>

            </ul>

            <div id="boatTabs1" class="tab-content">

                <fieldset>
                    <div class="row">
                        <section class="col col-xs-12">
                            @*<script src="~/jquery/jquery.js"></script>*@
                            @*<script src="~/jquery/jquery.min.js"></script>*@

                            <script type="text/javascript">
                                 //$("#Vessel").click(function () {
                                $(window).on('load',function () {
                              $("#tblBoat").jqGrid({
                                url: "/Services/GetBoats?informationId=@Model.AnswerSheetId&validated=true&removed=false&ceased=false&transfered=false",
                                datatype: "xml",
                                  mytpe: "GET",
                                async: true,
                                colNames: ["Edit Vessel", "Boat Name", "Year", "Sum Insured", "Delete Vessel"],
                                colModel: [
                                    //{ name: "edit", template: "actions", width: 60 },

                                    {
                                        name: "edit", template: "actions", width: 90, align: 'center', search: false, editable: true, formatter: function (cellvalue, options, rowobject) {
                                            return ('<button onclick="EditBoat(\'b\', \'' + cellvalue + '\'); SetCurrentTab(\'boatTabs2\');" type="button" class="btn btn-success btn-sm"><i class="fa fa-edit"> </i></button>')
                                        }
                                    },

                                    { name: "BoatName", align: 'center', key: 'true', editable: true },

                                    { name: "YearOfManufacture", align: 'center', editable: true },
                                    { name: "MaxSumInsured", align: 'center', editable: true },

                                    {
                                        name: "update", align: 'center', search: false,width:90, editable: true, formatter: function (cellvalue, options, rowobject) {
                                            return ('<button id="trash" onclick="SetBoatRemovedStatus(\'' + cellvalue + '\', \'true\', \'#tblBoat\', \'#tblRemovedBoat\');" type="button" class=" btn btn-danger btn-sm" ><span class="fa fa-trash" id="span"></span></button>');
                                        }
                                    }
                                ],
                                autowidth: true,
                                pager: "#pagerBoat",
                                rowNum: 10,
                                rowList: [10, 20, 30],
                                sortname: "BoatName",
                                sortorder: "desc",
                                viewrecords: true,
                                gridview: true,
                                autoencode: true,
                                onSelectRow: function (rowid) {
                                    var grid = $('#tblBoat');
                                    var myCellData = grid.jqGrid('getCell', rowid, 'column_name');
                                    //EditBoat('b', myCellData);
                                    SetCurrentTab("boatTabs-2");

                                }


                            });
                            $("#tblBoat").jqGrid('navGrid',
                                '#pagerBoat',
                                { edit: false, add: false, del: false },
                                //options
                                { height: 280, reloadAfterSubmit: false },
                                // edit options
                                { height: 280, reloadAfterSubmit: false },
                                // add options
                                { reloadAfterSubmit: false },
                                {search: true},
                                // del options
                                { sopt: ['eq', 'bw', 'cn'] }
                                // search options
                            );
                        });
                            </script>
                            <table id="tblBoat"><tr><td></td></tr></table>
                            <div id="pagerBoat"></div>
                        </section>
                    </div>
                </fieldset>
            </div>
            <div id="boatTabs2" class="tab-content">

                <fieldset id="fieldset">
                    <form data-toggle="validator" role="form" id="boatForm" class="needs-validation">
                        <input id="BoatId" class="b" name="BoatId" type="hidden">
                        <input id="openModal" class="b" name="openModal" type="hidden">
                        <!-- 1st group -->


                        <div class="row ">
                            @*<div class="form-group">
                            *@

                            <section class="col col-xs-12 col-sm-6 col-md-6 col-lg-6">
                                <label class="label required">Vessel Name</label>

                                <label class="input form-group">
                                    <i></i>
                                    <input class="form-control b " type="text" data-type="TEXTBOX" data-name="BoatName" id="BoatName" name="BoatName" placeholder="Vessel Name" value="" required>
                                </label>
                                <p class="validationContainer" id="validateBoatName"></p>

                            </section>


                            @*
                                </div>*@



                            <section class="col col-xs-12 col-sm-6 col-md-6 col-lg-6">

                                <label class="label required">Vessel Type</label>

                                <label class="input form-group">
                                    <select onchange="vesselType1Selected()" class="form-control b" id="BoatType1" name="BoatType1" title="" required>
                                        <option selected="selected" value="">-- Select here --</option>
                                        <option value="Powerboats">Powerboats</option>
                                        <option value="CruisersandLaunches">Cruisers and Launches</option>
                                        <option value="YachtsandCatamarans">Yachts and Catamarans</option>
                                        <option value="Jetski" id="BoatType1Jetski">Jetski</option>
                                        <option value="Jetboat">Jetboat</option>
                                        <option value="Other">Other Personal Watercraft</option>
                                    </select>

                                </label>
                                <p class="validationContainer" id="validateBoatType"></p>

                            </section>
                            <section class="col col-xs-12 col-sm-6 col-md-6 col-lg-6">
                                <label class="label required">Vessel status when not in use</label>
                                <div class="row">

                                    <section class="col col-xs-10 col-sm-10 col-md-10 col-lg-10">

                                        <label class="input form-group">
                                            <select onchange="vesselType2Selected()" class="form-control b" id="BoatType2" name="BoatType2" required>
                                                <option selected="selected" value="">-- Select --</option>
                                                <option value="Roadtrailer">Road Trailer</option>
                                                <option value="Stored">Stored</option>
                                                <option id="BoatType2Berthed" value="Berthed">Berthed</option>
                                                <option id="BoatType2Moored" value="Moored">Moored</option>
                                            </select>
                                        </label>
                                        <p class="validationContainer" id="validateBoatType2"></p>

                                    </section>
                                    <section class="col col-xs-1 col-sm-1 col-md-1 col-lg-1">
                                        <a href="#" data-toggle="popover" 
                                           data-trigger="hover"
                                           data-content="Please select the option that best represents the status of the vessel when it is not being used">
                                        <i class="fa fa-question-circle" style="width:15px"></i>
                                        </a>

                                        @*<i id="elem" href="#"
                                           class="fa fa-question-circle"
                                           data-placement="bottom"
                                           data-toggle="popover"
                                           data-container="body"
                                           rel="popover"
                                           data-content="Please select the option that best represents the status of the vessel when it is not being used"></i>*@
                                    </section>

                                </div>

                            </section>

                            <section class="col col-xs-12 col-sm-6 col-md-6 col-lg-6">
                                <label class="label required">Vessel Skipper</label>

                                <div class="row">
                                    <div class="col col-xs-10 col-sm-10 col-md-10 col-lg-10">

                                        <label class="input form-group">
                                            <select onchange="addNewSkipper()" class="form-control b" id="BoatOperator" name="BoatOperator" required>
                                                <option selected="selected" value="">-- Select --</option>
                                                @foreach (var boper in Model.SkipperList)
                                                {
                                                    <option value="@boper.Value">@boper.Text</option>
                                                }
                                                <option value="add">Add Skipper</option>
                                            </select>
                                        </label>
                                        <p class="validationContainer" id="validateBoatOperator"></p>

                                    </div>

                                    <section class="col col-xs-1 col-sm-1 col-md-1 col-lg-1">
                                        @*<a href="#" data-toggle="popover"
                                           data-trigger="hover"
                                           data-placement="bottom"
                                           data-content="Please select the option that best represents the status of the vessel when it is not being used">
                                            hint
                                        </a>*@

                                        <a href="#" data-toggle="popover"
                                           data-trigger="hover"
                                           title="" data-content="Please enter the details for the primary skipper of the vessel">
                                            hover on
                                        </a>

                                        @*<i id="elem" href="#"
        class="fa fa-question-circle"
        data-placement="bottom"
        data-toggle="popover"
        data-container="body"
        rel="popover"
        data-content="Please enter the details for the primary skipper of the vessel"></i>*@
                                    </section>

                                </div>


                            </section>

                            <div class="row">

                                <section class="col col-xs-12 col-sm-6 col-md-6 col-lg-6" id="RegistrationSection" hidden>

                                    <label class="label required">Personal Watercraft Registration Area</label>

                                    <label class="input form-group">
                                        <select class="form-control b" id="VesselArea" name="VesselArea" title="" required>
                                            <option selected="selected" value="">-- Select here --</option>
                                            <option value="Auckland">Auckland</option>
                                            <option value="Bay Of Plenty">Bay Of Plenty</option>
                                            <option value="Gisborne">Gisborne</option>
                                            <option id="Hawke's Bay">Hawke's Bay</option>
                                            <option value="Manawatu-Wanganui">Manawatu-Wanganui</option>
                                            <option value="NorthLand">Northland</option>
                                            <option value="Taranaki">Taranaki</option>
                                            <option value="Waikato">Waikato</option>
                                            <option id="Wellington">Wellington</option>
                                            <option value="Canterbury">Canterbury</option>
                                            <option value="Central Otag">Central Otago</option>
                                            <option value="Grey">Grey</option>
                                            <option id="Marlborough">Marlborough</option>
                                            <option value="Nelson">Nelson</option>
                                            <option value="Otago">Otago</option>
                                            <option value="Queenstown Lakes">Queenstown Lakes</option>
                                            <option id="Southland">Southland</option>
                                            <option value="Tasman">Tasman</option>
                                            <option value="West Coast">West Coast</option>
                                        </select>

                                    </label>
                                    <p class="validationContainer" id="validateVesselArea"></p>

                                </section>

                                <section class="col col-xs-12 col-sm-6 col-md-6 col-lg-6 hidden" id="aucklandRegoSection">
                                    <label class="label ">Personal Watercraft (PWC) Registration</label>
                                    <div class="row">

                                        <section class="col col-xs-10 col-sm-10 col-md-10 col-lg-10">


                                            <label class="input form-group">
                                                <i></i>
                                                <input class="form-control b" data-type="TEXTBOX" data-name="AucklandRegistration" id="AucklandRegistration" name="AucklandRegistration" placeholder="Personal Watercraft (PWC) Registration" value="">
                                            </label>
                                            <p class="validationContainer" id="validateAucklandRegistration"></p>

                                        </section>
                                        <section class="col col-xs-1 col-sm-1 col-md-1 col-lg-1">
                                            <a href="#" data-toggle="popover"
                                               data-trigger="hover"
                                               data-content="Registration of PWC including JetSki is required- for more information <a href='https://www.maritimenz.govt.nz/recreational/safety/regional-safety.asp' target='_blank' style='color:blue' title=''> click here </a> ">
                                                <i class="fa fa-question-circle" style="width:15px"></i>
                                            </a>

                                            @*<i id="elem" href="#"
                                               class="fa fa-question-circle"
                                               data-placement="bottom"
                                               data-container="body"
                                               data-html="true"
                                               rel="popover"
                                               data-content="Registration of PWC including JetSki is required- for more information <a href='https://www.maritimenz.govt.nz/recreational/safety/regional-safety.asp' target='_blank' style='color:blue' title=''> click here </a> "></i>*@
                                       
                                        </section>
                                    </div>
                                </section>

                            </div>


                        </div>
                        <hr class="boatAccordianHairline" />
                        <!-- 2nd group -->

                        <div class="row">

                            <section class="col col-xs-12 col-sm-6 col-md-6 col-lg-6">

                                <label class="label required">Is the vessel Professionally built?</label>
                                <div class="row">

                                    <div class="col col-xs-10 col-sm-10 col-md-10 col-lg-10">

                                        <label class="input form-group">
                                            <select class="form-control b" id="BuiltProfessionally" name="BuiltProfessionally" required>
                                                <option selected="selected" value="">-- Select --</option>
                                                <option value="True">Yes</option>
                                                <option value="False">No</option>
                                            </select>
                                        </label>
                                        <p class="validationContainer" id="validateBuiltProfessionally"></p>

                                    </div>
                                    <section class="col col-xs-1 col-sm-1 col-md-1 col-lg-1">
                                        <a href="#" data-toggle="popover"
                                           data-trigger="hover"
                                           data-content="Professionally built is defined as having been primarily constructed by a person or persons professionally engaged in the construction of watercraft ">
                                            <i class="fa fa-question-circle" style="width:15px"></i>
                                        </a>

                                        @*<i id="elem" href="#"
                                           class="fa fa-question-circle"
                                           data-placement="bottom"
                                           data-container="body"
                                           rel="popover"
                                           data-content="Professionally built is defined as having been primarily constructed by a person or persons professionally engaged in the construction of watercraft"></i>*@
                                  
                                    </section>


                                </div>
                            </section>



                            <section class="col col col-xs-12 col-sm-6 col-md-6 col-lg-6">

                                <label class="label required">Vessel Year of Manufacture</label>

                                <label class="input form-group">
                                    <i></i>
                                    <input class="form-control b" data-type="TEXTBOX" data-name="YearOfManufacture" id="YearOfManufacture" name="YearOfManufacture" placeholder="Vessel Year of Manufacture" value="" required>
                                </label>
                                <p class="validationContainer" id="validateYearOfManufacture"></p>

                            </section>


                            <section class="col col-xs-12 col-sm-6 col-md-6 col-lg-6">
                                <label class="label required">Vessel Make</label>
                                <label class="input form-group">
                                    <i></i>
                                    <input class="form-control b" data-type="TEXTBOX" data-name="BoatMake" id="BoatMake" name="BoatMake" placeholder="Vessel Make" value="" required>
                                </label>
                                <p class="validationContainer" id="validateBoatMake"></p>

                            </section>

                            <section class="col col-xs-12 col-sm-6 col-md-6 col-lg-6">
                                <label class="label required form-group">Vessel Model</label>
                                <label class="input form-group">
                                    <i></i>
                                    <input class="form-control b" data-type="TEXTBOX" data-name="BoatModel" id="BoatModel" name="BoatModel" placeholder="Vessel Model" value="" required>
                                </label>
                                <p class="validationContainer" id="validateBoatModel"></p>

                            </section>

                            <section class="col col-xs-12 col-sm-6 col-md-6 col-lg-6" id="HullConstructSection">
                                <label class="label required">Construction Type</label>

                                <div class="row">

                                    <div class="col col-xs-10 col-sm-10 col-md-10 col-lg-10">

                                        <label class="input form-group">
                                            <select onchange="hideShowHullConstructionOther()" class="form-control b" id="HullConstruction" name="HullConstruction" required>
                                                <option selected="selected" value="">-- Select --</option>
                                                <option value="Aluminium">Aluminium</option>
                                                <option value="Steel">Steel</option>
                                                <option value="Fibreglass">Fibreglass</option>
                                                <option value="Carbon">Carbon fibre</option>
                                                <option id="HullConstructionRigidHi" value="RigidHI">Rigid Hull Inflatable</option>
                                                <option id="HullConstructionWood" value="Wood">Wood/ timber</option>
                                                <option id="HullConstructionCement" value="Cement">Ferro cement/ cement</option>
                                                <option id="HullConstructionOther" value="Other">Other</option>
                                            </select>
                                        </label>
                                        <p class="validationContainer" id="validateHullConstruction"></p>

                                    </div>
                                    <section class="col col-xs-1 col-sm-1 col-md-1 col-lg-1">
                                        <a href="#" data-toggle="popover"
                                           data-trigger="hover"
                                           data-content="please select the primary construction material of the hull of your vessel">
                                            <i class="fa fa-question-circle" style="width:15px"></i>
                                        </a>

                                        @*<b id="elem" href="#"
                                           class="fa fa-question-circle"
                                           data-placement="bottom"
                                           data-container="body"
                                           rel="popover"
                                           data-content="please select the primary construction material of the hull of your vessel"></b>*@
                                    </section>

                                </div>

                            </section>


                            <section class="col col-xs-12 col-sm-6 col-md-6 col-lg-6 hidden" id="HullConstructOtherSection">
                                <label class="label required">Other Construction Type</label>
                                <label class="input ">
                                    <i></i>
                                    <input class="form-control b form-group" type="text" data-type="TEXTBOX" data-name="otherhullconstruction" id="OtherConstructionType" name="otherhullconstruction" placeholder="Other Construction Type" required>
                                </label>
                                <p class="validationContainer" id="validateHullConstructOtherSection"></p>

                            </section>
                        </div>

                        <div class="row">

                            <section class="col col-xs-12 col-sm-6 col-md-6 col-lg-6" id="HullConfigSection">
                                <label class="label required">Hull Configuration</label>
                                <label class="input form-group">
                                    <select onchange="hideShowHullConfigurationOther()" class="form-control b" id="HullConfiguration" name="HullConfiguration" required>
                                        <option selected="selected" value="">-- Select --</option>
                                        <option value="Mono">Mono</option>
                                        <option id="HullConfigurationCatamaran" value="Catamaran">Catamaran</option>
                                        <option id="HullConfigurationTrimaran" value="Trimaran">Trimaran</option>
                                        <option id="HullConfigurationOther" value="Other">Other</option>
                                    </select>
                                </label>
                                <p class="validationContainer" id="validateHullConfiguration"></p>

                            </section>

                            <section class="col col-xs-12 col-sm-6 col-md-6 col-lg-6 hidden" id="HullConfigurationOtherSection">
                                <label class="label required">Other Hull Configuration</label>
                                <label class="input ">
                                    <i></i>
                                    <input class="form-control b form-group" type="text" data-type="TEXTBOX" data-name="OtherHullConfiguration" id="OtherHullConfiguration" name="OtherHullConfiguration" placeholder="Other Hull Configuration" value="" required>
                                </label>
                                <p class="validationContainer" id="validateOtherHullConfiguration"></p>

                            </section>
                        </div>
                        <div class="row">

                            <section class="col col-xs-12 col-sm-6 col-md-6 col-lg-6">
                                <label class="label required">Motor Type</label>
                                <label class="input form-group">
                                    <select onchange="updateExcess()" class="form-control b" id="MotorType" name="MotorType" required>
                                        <option selected="selected" value="">-- Select --</option>
                                        <option id="MotorTypeInboardPetrol" value="InboardPetrol">Inboard – Petrol</option>
                                        <option id="MotorTypeInboardDiesel" value="InboardDiesel">Inboard – Diesel</option>
                                        <option id="MotorTypeInboardOther" value="InboardOther">Inboard – Other</option>
                                        <option id="MotorTypeOutboard" value="Outboard">Outboard</option>
                                        <option value="Jet">Jet</option>
                                        <option id="MotorTypeNA" value="NA">N/A</option>
                                    </select>
                                </label>
                                <p class="validationContainer" id="validateMotorType"></p>

                            </section>

                            <section class="col col-xs-12 col-sm-6 col-md-6 col-lg-6" id="ModifiedMotorSection">
                                <label class="label required">Has the motor been modified?</label>
                                <label class="input form-group">
                                    <select class="form-control b" id="ModifiedMotor" name="ModifiedMotor" required>
                                        <option selected="selected" value="">-- Select --</option>
                                        <option value="True">Yes</option>
                                        <option value="False">No</option>
                                    </select>
                                </label>
                                <p class="validationContainer" id="validateModifiedMotor"></p>

                            </section>

                            <section class="col col-xs-12 col-sm-6 col-md-6 col-lg-6">
                                <label class="label required">What is the maximum rated speed of the vessel?</label>
                                <label class="input form-group">
                                    <select class="form-control b" id="MaxRatedSpeed" name="MaxRatedSpeed" required>
                                        <option selected="selected" value="">-- Select --</option>
                                        <option value="Upto20Knots">Up to 20 Knots</option>
                                        <option value="Between20and60Knots">Between 20 and 60 Knots</option>
                                        <option value="Over60Knots">Over 60 Knots</option>
                                    </select>
                                </label>
                                <p class="validationContainer" id="validateMaxRatedSpeed"></p>

                            </section>


                        </div>

                        <hr class="boatAccordianHairline" />
                        <!-- 3rd group -->

                        <div class="row">

                            <section class="col col-xs-12 col-sm-6 col-md-6 col-lg-6" id="RiggingTypeSection">
                                <label class="label required">Rigging Type</label>
                                <label class="input form-group">
                                    <select class="form-control b" id="RiggingType" name="RiggingType" required>
                                        <option selected="selected" value="">-- Select --</option>
                                        <option value="StainlessSteelWire">Stainless Steel Wire</option>
                                        <option value="Rod">Rod</option>
                                        <option value="Other">Other</option>
                                    </select>
                                </label>
                                <p class="validationContainer" id="validateRiggingType"></p>

                            </section>

                            <section class="col col-xs-12 col-sm-6 col-md-6 col-lg-6" id="MastTypeSection">
                                <label class="label required">Mast Type</label>

                                <div class="row">

                                    <div class="col col-xs-10 col-sm-10 col-md-10 col-lg-10">

                                        <label class="input form-group">
                                            <select class="form-control b" id="MastType" name="MastType" required>
                                                <option selected="selected" value="">-- Select --</option>
                                                <option value="Aluminium">Aluminium</option>
                                                <option value="Steel">Steel</option>
                                                <option value="Wooden">Wooden</option>
                                                <option value="CarbonFibre">Carbon Fibre</option>
                                                <option value="Other">Other</option>
                                            </select>
                                        </label>
                                        <p class="validationContainer" id="validateMastType"></p>

                                    </div>
                                    <section class="col col-xs-1 col-sm-1 col-md-1 col-lg-1">
                                        <a href="#" data-toggle="popover"
                                           data-trigger="hover"
                                           data-placement="bottom"
                                           data-content="please select the primary construction material of the mast of your vessel">
                                            <i class="fa fa-question-circle" style="width:15px"></i>
                                        </a>

                                        @*<i id="elem" href="#"
                                           class="fa fa-question-circle"
                                           data-placement="bottom"
                                           data-container="body"
                                           rel="popover"
                                           data-content="please select the primary construction material of the mast of your vessel"></i>*@
                                    </section>

                                </div>
                            </section>

                            <section class="col col-xs-12 col-sm-6 col-md-6 col-lg-6">
                                <label class="label required">$ Vessel Insured Value (Please exclude trailer value)</label>
                                <label class="input form-group">
                                    <i></i>
                                    <input onchange="updateExcess()" class="form-control b" pattern="^\$\d{1,3}(,\d{3})*(\.\d+)?$" data-type="currency" data-name="MaxSumInsured" id="MaxSumInsured" name="MaxSumInsured" placeholder="$ Vessel Insured Value" value="" required>
                                </label>
                                <p class="validationContainer" id="validateMaxSumInsured"></p>

                            </section>

                            <section class="col col-xs-12 col-sm-6 col-md-6 col-lg-6">
                                <label class="label required">$ Excess</label>
                                <label class="input form-group">
                                    <select class="form-control b" id="excess" name="BoatQuoteExcessOption" required>
                                        <option selected="selected" value="">-- Select --</option>
                                        <option id="excess250" value="250">$250</option>
                                        <option id="excess500" value="500">$500</option>
                                        <option id="excess1000" value="1000">$1,000</option>
                                        <option id="excess2000" value="2000">$2,000</option>
                                    </select>
                                </label>
                                <p class="validationContainer" id="validateexcess"></p>

                            </section>
                        </div>

                        <div class="row">




                        </div>
                        <hr class="boatAccordianHairline" />
