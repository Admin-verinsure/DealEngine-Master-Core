@model TechCertain.WebUI.Models.Authorization.AuthorizeViewModel

<form>
    <div class="form-group col-6">
        <input type="text" class="form-control" id="InputRoleName" placeholder="Enter Role Name">
    </div>

    <div class="form-group col-6">
        <select class="cselect form-control" data-placeholder="Add Claims" id="AddClaims" multiple="multiple" name="@Html.NameFor(m => m.ClaimList)" style="display: none;">
            @foreach (var claim in Model.ClaimList)
            {
                <option value="@claim.Value">@claim.Value</option>
            }
        </select>
    </div>
    <div class="form-group col-6">
        <button type="button" class="btn btn-primary btn-lg" onclick="SaveRole()">Save</button>
    </div>
</form>


