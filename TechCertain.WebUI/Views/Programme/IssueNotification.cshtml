@model TechCertain.WebUI.Models.Programme.ProgrammeInfoViewModel
<!-- MAIN CONTENT -->
<div id="content" class="container-fluid">
    <div class="row">
        <h1 class="page-title txt-color-blueDark">
            <!-- PAGE HEADER -->
            <i class="fa-fw fa fa-file-o"></i>  <span>
                <a href="~/Programme/TermSheetConfirguration/@Model.Id">> Package Term Sheet</a>

                <a href="~/Programme/ManageRules/@Model.Id">> Manage Rules</a>
                > @Model.Name
            </span>
        </h1>
    </div>

    <div class="row">
        <span>
            <a onclick="location.href='@Url.Action("ManageRules", "Programme",new { Id =@Model.Id })'" style="color:dodgerblue;font-size:30px; cursor:pointer"> <span _ngcontent-c7="" class="icon-append fa fa-arrow-circle-left"></span>  </a>
        </span>
    </div>

    <div class="row">
        <article class="container-fluid">
            <!-- new widget -->
            <div class="jarviswidget jarviswidget-color-blue" id="wid-id-2" data-widget-editbutton="true" data-widget-colorbutton="false">
                <header>
                    <span class="widget-icon"> <i class="fa fa-envelope text-white"></i> </span>
                    <h2>@Model.Name </h2>
                </header>
                <!-- widget div-->
                <div>
                    <div class="widget-body no-padding smart-form">
                        <!-- content goes here -->
                        @Html.HiddenFor(m => m.Id)
                        <section class="col col-xs-12 col-sm-12 col-md-12 col-lg-12">
                            <div class="row">
                            </div>
                        </section>
                        <section class="col col-xs-12 col-sm-6 col-md-6 col-lg-6" id="HullConfigSection">

                            <div class="row">

                                <section class="col col-xs-4 col-sm-4 col-md-4 col-lg-4">
                                    <label class="label required">Select Party:</label>

                                </section>
                                <section class="col col-xs-8 col-sm-8 col-md-8 col-lg-8">

                                    <label class="input form-group">
                                        <select onchange="selectorgUser()" class="form-control b" id="selectParty" name="selectedparty" required>
                                            <option selected="selected" value="">-- Select --</option>
                                            @foreach (var blloc in Model.Parties)
                                            {
                                                <option value="@blloc.Id"> @blloc.Name</option>
                                            }
                                        </select>
                                    </label>

                                </section>
                            </div>


                            <div class="row">

                                <section class="col col-xs-12 col-sm-10 col-md-10 col-lg-10" id="OrgUserSection">
                                    <div id="div1"></div>
                                </section>
                            </div>

                            <div class="row">
                                <section class="col col-xs-12 col-sm-1 col-md-1 col-lg-1">
                                    <div></div>
                                </section>

                                <button type="submit" class="btn btn-success btn-sm" onclick='addselectorgUser("@Model.Name")'>save</button>
                            </div>
                        </section>


                    </div>
                </div>
            </div>
        </article>
    </div>
</div>


@section pagespecific {

    @Html.Partial("_ProgrammeScripts")

}
